{
    "singleQuote": false,
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": true,
    "printWidth": 120,
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "endOfLine": "auto",
    "jsxBracketSameLine": false,
    "useTabs": false,
    "vueIndentScriptAndStyle": true,
    "htmlWhitespaceSensitivity": "strict",
    "quoteProps": "consistent",
    "requirePragma": false,
    "insertPragma": false,
    "proseWrap": "never"
}